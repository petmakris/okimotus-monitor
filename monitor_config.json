{
  "title": "Phase Tracker",
  "refresh_rate": 100,
  "window": {
    "width": 1000,
    "height": 600
  },
  "fields": {
    "0": {
      "label": "Time",
      "type": "int",
      "format": "{:,}",
      "unit": "counts",
      "color": "blue",
      "transformations": [
        {
          "label": "Seconds",
          "operation": "divide",
          "value": 1000,
          "format": "{:.3f}",
          "unit": "s"
        }
      ]
    },
    "1": {
      "label": "Encoder 1",
      "type": "int",
      "format": "{:,}",
      "unit": "counts",
      "color": "green",
      "transformations": [
        {
          "label": "Rotations",
          "operation": "divide",
          "value": 1600,
          "format": "{:.3f}",
          "unit": "rev"
        },
        {
          "label": "Degrees",
          "operation": "multiply",
          "value": 0.225,
          "format": "{:.1f}",
          "unit": "\u00b0"
        }
      ]
    },
    "2": {
      "label": "Encoder 2",
      "type": "int",
      "format": "{:,}",
      "unit": "counts",
      "color": "red",
      "transformations": [
        {
          "label": "Rotations",
          "operation": "divide",
          "value": 4096,
          "format": "{:.3f}",
          "unit": "rev"
        }
      ]
    }
  }
}